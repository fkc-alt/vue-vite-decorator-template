{
  "name": "vite-project",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite --mode=dev",
    "test": "vue-tsc --noEmit && vite build --mode=test",
    "build": "vue-tsc --noEmit && vite build --mode=prod",
    "dts": "vue-tsc --emitDeclarationOnly --declaration",
    "preview": "vite preview",
    "lint": "eslint {src,typings,mock,types}/**/*.{ts,d.ts,tsx,vue} vite.config.ts --no-error-on-unmatched-pattern --fix",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.6",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "element-plus": "^2.2.19",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.16",
    "reflect-metadata": "^0.1.13",
    "vue": "^3.2.45",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@iconify-json/ep": "^1.1.8",
    "@intlify/unplugin-vue-i18n": "^0.4.0",
    "@swc/core": "^1.3.32",
    "@types/node": "^18.6.1",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-icons": "^0.14.12",
    "unplugin-swc": "^1.3.2",
    "unplugin-vue-components": "^0.22.12",
    "unplugin-vue-define-options": "^0.6.2",
    "vite": "^3.0.0",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-progress": "0.0.6",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-images": "^0.6.1",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.9"
  },
  "lint-staged": {
    "{src,typings,mock}/**/*.{ts,d.ts,vue,tsx}": [
      "eslint --fix"
    ],
    "vite.config.ts": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
